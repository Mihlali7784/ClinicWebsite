
@{
    ViewData["Title"] = "Details";
    Layout = "~/Views/Shared/_PatientMedicalProcedureLayout.cshtml";
}
@model PHCApplication.Models.Reminder
<h1>Reminder Details</h1>

<<div>
    <h4>Booking</h4>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.ReminderType)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.ReminderType)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.ReminderDate)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.ReminderDate)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Description)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Description)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.IsCompleted)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.IsCompleted)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Priority)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Priority)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.IsRecurring)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.IsRecurring)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.ApplicationUser)
        </dt>
        <dd class="col-sm-10">
            @*@Html.DisplayFor(model => model.ApplicationUser.Id)*@
            @Html.DisplayFor(model => model.ApplicationUser.FirstName)
            @Html.DisplayFor(model => model.ApplicationUser.LastName)
        </dd>
    </dl>
